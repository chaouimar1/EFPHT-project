{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Equipement</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Isdisponible</th>
                <td>{{ entity.isDisponible }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ entity.nombre }}</td>
            </tr>
            <tr>
                <th>Nombre_endommage</th>
                <td>{{ entity.nombreendommage }}</td>
            </tr>
            <tr>
                <th>Nombre_nutilisable</th>
                <td>{{ entity.nombrenutilisable }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{{ entity.created|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('equipement') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('equipement_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
