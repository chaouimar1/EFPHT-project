{% extends "PFEDashBundle::layout.html.twig" %}

{% block title %}
    CRUD - {{ parent() }}
{% endblock %}

{% block body -%}
    <div class="row card-panel wtitle top-teal col offset-s1 s10 crudcard">
        <h5 class="card-title">Pret creation</h5>
        {{ form_start(form) }}
        <div class="row">
            <div class="input-field col s6">
                {{ form_row(form.description) }}
            </div>
            <div class="input-field col s6">
                {{ form_row(form.nombre) }}
            </div>
            <div class="input-field col s6">
                {{ form_row(form.typepret) }}
            </div>
            <div class="input-field col s6">
                {{ form_row(form.typefondoc) }}
            </div>
            {{ form_widget(form) }}
            <a href="{{ path('pret') }}" class="btn blue-grey">
                <i class="mdi-action-view-headline left"></i> Retour Ã  la liste
            </a>
            {{ form_end(form) }}
        </div>
    </div>
{% endblock %}
